<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="191.0" prefWidth="295.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.chess.MainViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Multiplayer Chess!">
      <font>
         <Font name="Arial Bold" size="15.0" />
      </font>
   </Label>
   <Button fx:id="localPlay" mnemonicParsing="false" onAction="#handlePlayLocal" text="Play Locally " />
    <Button fx:id="hostGame" text="Host Game" />
   <Button fx:id="connectToServer" text="Connect to Host" />
</VBox>
